"Vertexlitgeneric"
{
	
	
	"$basetexture" "models\characters\Mutants\minor\sack"
	"$surfaceprop" "alienflesh"
	"$bumpmap" "models\characters\Mutants\minor\sack_normal"
	"$model" 1
	"$selfillum" 1
	"$selfillumtint" "[0.25 0.00 0.05]"
	"$normalmapalphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.05 0.05 0.05]"
	"$envmapconstrast" "1"
	"$envmapsaturation" 1
	
	"Proxies"
	{
		"Sine"
		{
			"resultVar" "$selfillumtint[0]"
			"sineperiod" 3
			"sinemin" .75
			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$selfillumtint[1]"
			"sineperiod" 3
			"sinemin" .65
			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$selfillumtint[2]"
			"sineperiod" 3
			"sinemin" .85
			"sinemax" 1
		}

			
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 15.00
		}
	}
}






